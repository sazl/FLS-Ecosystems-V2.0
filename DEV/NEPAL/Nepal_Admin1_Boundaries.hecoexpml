	$artefact.addAttribute("REG_NAME", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "REG_NAME", false)));
	$artefact.addAttribute("Centroid_X", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "Centroid_X", false)));
	$artefact.addAttribute("Centroid_Y", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "Centroid_Y", false)));
	$artefact.addAttribute("Y_Max", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "Y_Max", false)));
	$artefact.addAttribute("Y_Min", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "Y_Min", false)));
	$artefact.addAttribute("X_Max", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "X_Max", false)));
	$artefact.addAttribute("X_Min", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "X_Min", false)));
	$artefact.addAttribute("HRName", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "HRName", false)));
	$artefact.addAttribute("HRPCode", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "HRPCode", false)));
	$artefact.addAttribute("HRParent", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "HRParent", false)));


            
}

String getName() {
	return $artefact.getAttributeValueAsString("REG_NAME_S");
